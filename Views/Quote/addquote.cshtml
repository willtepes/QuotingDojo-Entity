<h1>Welcome to QuotingDojo</h1>
        Your name: @ViewBag.first_name @ViewBag.last_name<br>
        @model quotingdojo3.Models.Registerquote
        <form asp-controller="Quote" asp-action="quotes" method="post" role="form">
            <div style="color:red" asp-validation-summary="All"></div>
            Your quote: <textarea asp-for="quote" cols="30" rows="10"></textarea> <br>
            <input type="submit" name="Add my quote!">
        </form>
        <div>
    </div>
        <a href="allquotes">Skip to quotes!</a><br><br>      
        <button><a href="logout">Log Out</a> </button>